{
    "compilerOptions": {
        "target": "es6",
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true,
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "moduleResolution": "node",
        "lib": [
            "es6",
            "dom"
        ],
        "types": [
            "cypress"
        ]
    },
    "include": [
        "**/*.js",
        "**/*.ts",
        "../node_modules/cypress",
        "support/commands.jts"
    ]
}